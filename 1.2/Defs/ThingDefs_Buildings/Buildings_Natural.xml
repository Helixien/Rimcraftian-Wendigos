<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef ParentName="RockBase">
        <defName>RCW_BonePiles</defName>
        <label>bone piles</label>
        <description>Collapsed rocks, packed tightly enough to hold up a roof.</description>
        <graphicData>
            <texPath>Things/Building/Natural/BonePile</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <drawSize>(1,1)</drawSize>
        </graphicData>
        <statBases>
            <MaxHitPoints>15</MaxHitPoints>
        </statBases>
        <rotatable>false</rotatable>
        <building>
            <claimable>true</claimable>
        </building>
    </ThingDef>

</Defs>